.fui-card__wrap[data-v-745c6b3e] {
  flex: 1;
  box-sizing: border-box;
  overflow:hidden
}

.fui-card__full[data-v-745c6b3e] {
  margin-left: 0 !important;
  margin-right: 0 !important;
  border-radius:0 !important
}

.fui-card__full[data-v-745c6b3e]:after {
  border-radius:0 !important
}

.fui-card__border[data-v-745c6b3e] {
  position: relative;
  box-shadow:none !important
}

.fui-card__border-radius[data-v-745c6b3e] {
  border-radius:.5rem !important
}

.fui-card__border[data-v-745c6b3e]:after {
  content: " ";
  position: absolute;
  height: 200%;
  width: 200%;
  border: 1px solid var(--fui-color-border, #EEEEEE);
  transform-origin: 0 0;
  -webkit-transform-origin: 0 0;
  transform: scale(.5);
  left: 0;
  top: 0;
  border-radius: 1rem;
  box-sizing: border-box;
  pointer-events:none
}

.fui-card__header[data-v-745c6b3e] {
  width: 100%;
  display: flex;
  box-sizing: border-box;
  overflow: hidden;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  position:relative
}

.fui-card__header-left[data-v-745c6b3e] {
  display: flex;
  white-space: nowrap;
  flex-direction: row;
  align-items:center
}

.fui-card__header-line[data-v-745c6b3e] {
  position:relative
}

.fui-card__header-line[data-v-745c6b3e]:after {
  content: "";
  position: absolute;
  border-bottom: 1px solid var(--fui-color-border, #EEEEEE);
  transform: scaleY(.5);
  transform-origin: 0 100%;
  bottom: 0;
  right: 0;
  left: 0;
  pointer-events:none
}

.fui-card__header-thumb[data-v-745c6b3e] {
  vertical-align: middle;
  margin-right: .625rem;
  flex-shrink:0
}

.fui-card__header-title[data-v-745c6b3e] {
  display: inline-block;
  vertical-align: middle;
  max-width: 11.875rem;
  overflow: hidden;
  white-space: nowrap;
