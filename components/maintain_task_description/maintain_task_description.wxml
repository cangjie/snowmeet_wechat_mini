<view  class="flex-wrp" style="flex-direction:row;height:130rpx;margin-top:0rpx;margin-left:10rpx;margin-top:10rpx">
  <view class="flex-wrp" style="flex-direction:column;height:120rpx;margin-top:1rpx">
    <view style="height:105rpx">
      <image style="width:100rpx;height:100rpx" src="{{userInfo.head_image}}"></image>
    </view>
    <view style="margin-top:1rpx;font-size:12px" >{{maintainTask.contact_name}}</view>
  </view>
  <view style="width:500rpx;margin-left:10rpx;flex-direction:column" >
    <view class="flex-wrp" style="font-size:12px;flex-direction:row;margin-top:0rpx">
      <view style="width:250rpx" >订单号: {{maintainTask.id}}</view> 
      <view style="width:250rpx" >下单日期: {{orderDate}}</view>
    </view>
    <view class="flex-wrp" style="font-size:12px;flex-direction:row;margin-top:0rpx">
      <view style="width:250rpx" >下单方式: {{maintainTask.order_type}}</view> 
      <view style="width:250rpx">下单时间: {{orderTime}}</view>
    </view>
    <view class="flex-wrp" style="font-size:12px;flex-direction:row;margin-top:0rpx">
      <view style="width:250rpx" >套餐金额: {{price}}</view> 
      <view style="width:250rpx">套餐卡号: {{maintainTask.card_no}}</view>
    </view>
    <view class="flex-wrp" style="font-size:12px;flex-direction:row;margin-top:0rpx">
      <view style="width:250rpx" >装备类型: {{userFilledInfo.equip_type}}</view> 
      <view style="width:250rpx">业务类型: 装备养护</view>
    </view>
  </view>
</view>